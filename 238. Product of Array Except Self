class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        n = len(nums)
        ans = [1]*n
        prod = 1
        for i in range(n):
            ans[i]*=prod
            prod*=nums[i]
        prod=1
        for i in range(n-1,-1,-1):
            ans[i]*=prod
            prod*=nums[i]
        return ans
